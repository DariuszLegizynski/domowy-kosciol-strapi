{
  "kind": "collectionType",
  "collectionName": "galleries",
  "info": {
    "singularName": "gallery",
    "pluralName": "galleries",
    "displayName": "Gallery",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "description": {
      "type": "string",
      "maxLength": 255
    },
    "shortDescription": {
      "type": "string",
      "required": false,
      "maxLength": 80,
      "default": "Rekolekcje z warsztatów odnowy w Duchu Św w Wądolicach górnych"
    },
    "coverImage": {
      "type": "component",
      "repeatable": false,
      "component": "base.base-image"
    }
  }
}
